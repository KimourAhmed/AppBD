package tables;


public class Cadre extends Impression {
	int idCadre;
	String reference;
	
	
	public Cadre(int idCadre, String reference) {	
		super(idCadre, reference);
	}
	
	public Cadre() {
	}
	
	
	
	
}

public class Tirage extends Impression{
	
	public Tirage(int idTir, String reference) {
		super (idTir, reference);
	}
	
	public Tirage() {
		
	}

	public int getIdTir() {
		return getIdImpr();
	}

	public void setIdTir(int idTir) {
		this.setIdImpr(idTir);
	}

	public String getReference() {
		return getReference();
	}

	public void setReference(String reference) {
		this.setReference(reference);
	}
	
}
